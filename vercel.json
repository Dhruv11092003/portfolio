{
    "projects": [
      {
        "src": "project-backend/package.json",
        "use": "@vercel/node",
        "config": {
          "distDir": "project-backend"
        }
      },
      {
        "src": "project-front/package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "project-front/build"
        }
      }
    ],
    "routes": [
      { "src": "/api/(.*)", "dest": "/project-backend/$1" },
      { "src": "/(.*)", "dest": "/project-front/$1" }
    ]
  }
      